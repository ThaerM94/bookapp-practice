<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <%console.log(data)%>
    <%data.forEach(data=>{%>
        <h1><%=data.title%></h1>
        <h3><%=data.authors%></h3>
        <img src="<%=data.img%>" alt="">
        <p><%=data.description%></p>
        <a href="/details/<%=data.id%>"><button>details</button></a>
    <%})%>
</body>
</html>